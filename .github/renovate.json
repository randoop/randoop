    "config:base",
    ":automergeAll",
    ":automergeRequireAllStatusChecks",
    "schedule:nonOfficeHours",
    ":disableDependencyDashboard"
  ],
  "timezone": "America/Los_Angeles",
  "packageRules": [
    {
      "matchPackageNames": ["reflection-util"],
      "allowedVersions": "!/^1\.1\.4$/"
    }
  ]
}
