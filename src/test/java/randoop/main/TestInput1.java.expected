package test;

import org.junit.FixMethodOrder;
import org.junit.Test;
import org.junit.runners.MethodSorters;
import org.apache.commons.lang3.math.Fraction;

@FixMethodOrder(MethodSorters.NAME_ASCENDING)
public class TestInput1Minimized {

    public static boolean debug = false;

    @Test
    public void test1() throws Throwable {
        Fraction fraction0 = Fraction.ONE_QUARTER;
        Fraction fraction1 = Fraction.TWO_QUARTERS;
        Fraction fraction4 = fraction0.divideBy(fraction1);
        // Checks the contract: compareTo-equals on fraction1 and fraction4
        org.junit.Assert.assertTrue("Contract failed: compareTo-equals on fraction1 and fraction4", (fraction1.compareTo(fraction4) == 0) == fraction1.equals(fraction4));
    }
}
